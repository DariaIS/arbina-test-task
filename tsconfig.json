{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "types": ["node"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@redux": [
        "./src/redux"
      ],
      "@UIElements/*": [
        "./src/components/UIElements/*"
      ],
      "@UIElements": [
        "./src/components/UIElements"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@pages": [
        "./src/pages"
      ],
      "@types": [
        "types"
      ]
    },
    "incremental": true
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
